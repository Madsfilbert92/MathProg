 108      z 'max profit'
 109      ;
 110   
 111  integer variables
 112       x(i)     'Produced of product i in 1000'
 113       sol(i,j) 'sold product i in region j in 10000'
 114       t(k)    'Timber assortment for timber k in 10000'
 115       s(k)    'Material surplus timber k in 1000';
 116   
 117   
 118  equations
 119          profit                  'objective function'
 120          materialReq     ''
 121          sawMillCap              ''
 122          plywoodMillCap  ''
 123          line1Cap                ''
 124          line2Cap                ''
 125          paperMillCap    ''
 126          surPlus(k)              ''
 127          SoldLessThanProduced ''
 128          HSELToSell      ''
 129          LSELToSell      ''
 130          ;
 131   
 132                  profit ..                       z =e= -sum((i,j), ((demand(i,j,'Gamma')-demand(i,j,'Delta')*(sol(i,j)*10)) - c(i)*x(i))) +
 133                                    sum(k, (cost(k,'Alpha')+cost(k,'Beta')*t(k)*10)) +
 134                                    sum(fp, 0.2*x(fp)*40) +
 135                                    sum(k,s(k)*cost(k,'Alpha'))
 136                                    ;
 137                  sawMillCap..            sum((sm), x(sm)) =l= 200;
 138                  plywoodMillCap..        sum((pm), x(pm)) =l= 90;
 139                  line1Cap..                      x('HSEL') =l= 220;
 140                  line2Cap..                      x('LSEL') =l= 180;
 141                  paperMillCap..          x('PAP') =l= 80;
 142          surPlus(k)..        t(k)*10 - sum((i,j), x(i)*ProductReq(i,k)) =e= s(k);
 143                  materialReq(k) ..   sum(i, ProductReq(i,k)*x(i)) =l= t(k)*10;
 144          SoldLessThanProduced(i) .. sum(j, sol(i,j)) =l=  x(i)/10;
 145          HSELToSell .. sum(j, sol('HSEL', j)) =e= (x('HSEL')-0.2*x('PAP'))/10;
 146          LSELToSell .. sum(j, sol('LSEL', j)) =e= (x('LSEL')-0.2*x('PAP'))/10;
 147   
 148  model aStaticModel /all/ ;
 149   
 150  solve aStaticModel using mip maximizing z;
 151   
 152  Display x.L, t.L, sol.L, x.M;


